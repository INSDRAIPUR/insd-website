 <script>
        function openHeroModal() {
            const modal = document.getElementById('heroVideoModal');
            const iframe = modal.querySelector('iframe');
            // Set autoplay to start video when modal opens
            iframe.src = iframe.src.replace('autoplay=0', 'autoplay=1');
            modal.style.display = 'flex';
            // Prevent body scrolling when modal is open
            document.body.style.overflow = 'hidden';
        }

        function closeHeroModal() {
            const modal = document.getElementById('heroVideoModal');
            const iframe = modal.querySelector('iframe');
            // Stop video by resetting src to disable autoplay
            iframe.src = iframe.src.replace('autoplay=1', 'autoplay=0');
            modal.style.display = 'none';
            // Restore body scrolling when modal is closed
            document.body.style.overflow = 'auto';
        }

        // Close modal when clicking outside the video
        document.getElementById('heroVideoModal').addEventListener('click', function(event) {
            if (event.target === this) {
                closeHeroModal();
            }
        });
    </script>
